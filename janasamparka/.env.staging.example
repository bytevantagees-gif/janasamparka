# Staging Environment Configuration
# Copy this file to .env.staging and fill in actual values

# Application
APP_NAME=Janasamparka API (Staging)
ENVIRONMENT=staging
DEBUG=true

# Database - Staging PostgreSQL
DATABASE_URL=postgresql://janasamparka:staging-password@staging-db-host:5432/janasamparka_staging

# Security - Use staging-specific keys
SECRET_KEY=staging-secret-key-not-for-production-but-secure-enough
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60
REFRESH_TOKEN_EXPIRE_MINUTES=10080

# SMS Provider - Staging
SMS_PROVIDER_API_KEY=staging-sms-api-key

# CORS Origins - Staging
CORS_ORIGINS=https://staging.janasamparka.karnataka.gov.in,http://localhost:3000

# External API Keys - Staging
BHOOMI_API_KEY=staging-bhoomi-api-key
KSNDMC_API_KEY=staging-ksndmc-api-key
APMC_API_KEY=staging-apmc-api-key

# Google Cloud Services - Staging
GOOGLE_CLOUD_PROJECT=janasamparka-staging
GOOGLE_APPLICATION_CREDENTIALS=/path/to/staging-service-account.json

# Firebase - Staging
FIREBASE_CREDENTIALS_PATH=/path/to/staging-firebase-credentials.json
FIREBASE_PROJECT_ID=janasamparka-staging

# AWS S3 - Staging
AWS_ACCESS_KEY_ID=staging-aws-access-key
AWS_SECRET_ACCESS_KEY=staging-aws-secret-key
AWS_S3_BUCKET=janasamparka-staging-files
AWS_REGION=ap-south-1

# Redis - Staging
REDIS_URL=redis://staging-redis-host:6379/0

# Monitoring
SENTRY_DSN=https://staging-sentry-dsn@sentry.io/project-id
LOG_LEVEL=DEBUG
LOG_FORMAT=json

# Email - Staging
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=staging-email@gmail.com
SMTP_PASSWORD=staging-app-password
SMTP_USE_TLS=true

# Performance
ENABLE_CACHING=true
CACHE_TTL_DEFAULT=300
CACHE_TTL_LONG=3600
